{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "typecheck": "tsc -b",
    "gen": "eval $(grep SUPABASE_ID .env) && supabase gen types typescript --project-id $SUPABASE_ID > ./app/types.generated.ts",
    "format": "prettier --write 'app/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@remix-run/node": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "@remix-run/vercel": "^1.9.0",
    "@supabase/auth-helpers-remix": "^0.1.3",
    "@supabase/supabase-js": "^2.2.1",
    "@vercel/node": "^2.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.9.0",
    "@remix-run/eslint-config": "^1.9.0",
    "@remix-run/serve": "^1.9.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.8.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "supabase": "^1.27.7",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
